{% set taxlist = taxonomylist.get() %}

{% if taxlist %}

<ul class="archives">
    {% for tax,value in taxlist[taxonomy] %}
    <li>
        <a href="{{ base_url }}/tag{{ config.system.param_sep }}{{ tax|e('url') }}">{{ tax }}  <span>({{ tax|length }})</span></a>
    </li>
    {% endfor %}
</ul>
{% endif %}
